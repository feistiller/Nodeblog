<%- include header%>
<form method="post">
    lable:<br>
    <input type="text" name="title" value="<%= post.title%>" disabled="disabled"/><br>
    tag:<br><input type="text" value="<%=post.tags[0]%>" name="tag1"/> <input type="text" value="<%=post.tags[1]%>" name="tag2"/><input type="text" value="<%=post.tags[2]%>" name="tag3"/><br>
    text:<br>
    <textarea name="post" rows="20" cols="100"><%= post.post%></textarea><br>
    <input type="submit" value="save">
</form>
<%- include footer%>